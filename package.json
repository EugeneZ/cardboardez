{
  "name": "cardboardez",
  "description": "CardboardEZ is a tool that helps simulate various tabletop games.",
  "author": "Eugene Zaretskiy",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "browserify -t babelify client/main.js > public/assets/scripts/client.js && node server.js",
    "watchify": "watchify --debug -t babelify client/main.js -o public/assets/scripts/client.js",
    "nodemon": "nodemon --debug --ignore client/ --ignore public/ --ignore game/trixit/component/ server.js"
  },
  "dependencies": {
    "autobind-decorator": "^1.3.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.13.0",
    "feathers": "^2.0.2",
    "feathers-authentication": "^0.7.10",
    "feathers-hooks": "^1.5.7",
    "feathers-socketio": "^1.4.1",
    "lodash": "^4.16.2",
    "material-ui": "^0.15.4",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.6.0",
    "redux-saga": "^0.11.1",
    "socket.io-client": "^1.4.8"
  },
  "devDependencies": {
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-preset-latest": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "body-parser": "^1.10.2",
    "config": "^1.10.0",
    "config-heroku": "^0.1.1-a",
    "crypto-js": "^3.1.6",
    "feathers-errors": "^2.4.0",
    "feathers-rest": "^1.5.0",
    "feathers-rethinkdb": "^0.2.3",
    "nodemon": "^1.10.2",
    "passport-facebook": "^1.0.3",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^1.0.0",
    "rethinkdbdash": "^2.3.23"
  }
}
